{
  "name": "@waypoint/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Waypoint token routing on Aptos",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "waypoint",
    "aptos",
    "token",
    "streaming",
    "payment",
    "vesting",
    "sdk"
  ],
  "author": "Waypoint",
  "license": "MIT",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.29.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.10",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
