{
  "name": "@compx/waypoint-sdk",
  "version": "1.4.2",
  "description": "TypeScript SDK for Waypoint token routing on Aptos and Algorand",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "example:aptos": "tsx examples/aptos-node.ts",
    "example:algo-linear": "tsx examples/algorand-linear-node.ts",
    "example:algo-invoice": "tsx examples/algorand-invoice-node.ts"
  },
  "keywords": [
    "waypoint",
    "aptos",
    "algorand",
    "token",
    "streaming",
    "payment",
    "vesting",
    "sdk"
  ],
  "author": "Waypoint",
  "license": "MIT",
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^9.1.2",
    "@aptos-labs/ts-sdk": "^5.1.5",
    "algosdk": "^3.5.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
